<% if @payment_link.errors.present? %>
  toastr.error("<%= @payment_link.errors.full_messages.to_sentence %>")
<% else %>
  $('#payment_link_<%= @payment_link.id%>').html("<%= escape_javascript render( partial: 'agency/payment_links/shared/payment_link', locals: {payment_link: @payment_link, plan: @payment_link.plan}) %>");
<% end %>
