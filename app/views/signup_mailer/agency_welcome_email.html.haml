          margin: 30px auto;
          border-radius: 4px;
          display: table;
      }

      .subhead {
          color: #555555;
          font-family: 'Montserrat', sans-serif;
          font-size: 20px;
          font-weight: 400;
          margin: 0;
          padding: 0;
          text-align: left;
      }

      .sub-title {
          color: #555555;
          font-family: 'Montserrat', sans-serif;
          font-weight: 600;
      }

      .subhead--sm {
          font-size: 16px;
          line-height: 1.2em;
      }

      .mhead {
          color: #333333;
          font-family: 'Montserrat', sans-serif;
          font-size: 23px;
          font-weight: 600;
          margin: 0;
          padding: 0;
          text-align: left;
      }

      .f-head {
          color: #333333;
          font-family: 'Montserrat', sans-serif;
          font-size: 19px;
          font-weight: 600;
          margin: 0;
          padding: 0;
          text-align: left;
      }

      .cellpadd10 {
          display: table;
          padding: 10px 20px;
      }

      .cellpadd-t-20 {
          display: table;
          padding: 20px 20px;
      }

      .link-a {
          color: #2196F3;
          font-family: 'Montserrat', sans-serif;
          font-size: 18px;
          text-decoration: none;
      }

      .link-a:hover {
          color: #4aafff;
          text-decoration: none;
      }

      .divider {
          border-bottom: 1px solid #e0e0e0;
          display: block;
          margin: 0 auto;
          width: 90%;
      }

      .margt-20 {
          margin-top: 20px !important
      }

      .txtcenter {
          text-align: center !important
      }

      .head-st-2 {
          padding: 20px 0;
          font-size: 24px;
          font-weight: 600;
          line-height: 40px;
      }

      .lnh-n {
          line-height: 1.6em;
          letter-spacing: 0px;
          padding-bottom: 20px;
      }

      .link-2 {
          color: #F8B018;
          font-weight: 600px;
          border-bottom: dashed 1px #f0f0f0
      }

      .link-2:hover {
          border-bottom: solid 1px #f0f0f0;
          text-decoration: none
      }

      @media only screen and (max-width:640px) {

          table table,
          td[class=full_width] {
              width: 100% !important;
          }
      }

      @media only screen and (max-width:640px) {

