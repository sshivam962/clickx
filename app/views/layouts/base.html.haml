!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge", "http-equiv" => "X-UA-Compatible"}/
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}
    %meta{content: 'noindex', name: 'robots'}
    %title= yield(:title) || 'Clickx'
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => false
    %link{:href => "//maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css", :rel => "stylesheet"}/
    = stylesheet_link_tag "https://www.authy.com/form.authy.min.css"
    = stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.10/css/select2.min.css'
    = stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/ekko-lightbox/5.3.0/ekko-lightbox.css'
    = stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery-minicolors/2.3.4/jquery.minicolors.min.css'
    %link{crossorigin: 'anonymous', href: 'https://cdnjs.cloudflare.com/ajax/libs/jquery-tagsinput/1.3.6/jquery.tagsinput.min.css', integrity: 'sha256-j5X3Yk4AwOdSE1go4GG6eQ/Bq0+gY2STs+GlULjQ+y8=', rel: 'stylesheet'}
    %link{crossorigin: 'anonymous', href: 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css', integrity: 'sha256-ENFZrbVzylNbgnXx0n3I1g//2WeO47XxoPe0vkp3NC8=', rel: 'stylesheet'}

    = javascript_include_tag "https://www.authy.com/form.authy.min.js"
    = javascript_include_tag Ckeditor.cdn_url
    = javascript_include_tag 'application', 'data-turbolinks-track' => false
    = javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.10/js/select2.min.js'
    = javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/ekko-lightbox/5.3.0/ekko-lightbox.min.js'
    = javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery-minicolors/2.3.4/jquery.minicolors.min.js'
    %script{crossorigin: 'anonymous', integrity: 'sha256-IkytFcY/Z/rLvRE9rYyZvybaitAWr1y2jS/+eyxXky8=', src: 'https://cdnjs.cloudflare.com/ajax/libs/jquery-tagsinput/1.3.6/jquery.tagsinput.min.js'}
    %script{crossorigin: 'anonymous', integrity: 'sha256-3blsJd4Hli/7wCQ+bmgXfOdK7p/ZUMtPXY08jmxSSgk=', src: 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js'}
    %script{:src => "https://cdn.jsdelivr.net/momentjs/latest/moment.min.js", :type => "text/javascript"}

    %script{:src => "https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js", :type => "text/javascript"}

    = javascript_include_tag '//cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js'
    = javascript_include_tag '//cdn.datatables.net/1.10.19/js/dataTables.bootstrap.min.js'
    = stylesheet_link_tag '//cdn.datatables.net/1.10.19/css/dataTables.bootstrap.min.css'

    %link{:href => "https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css", :rel => "stylesheet", :type => "text/css"}

    %script{:crossorigin => "anonymous", :integrity => "sha512-T/tUfKSV1bihCnd+MxKD0Hm1uBBroVYBOYSk1knyvQ9VyZJpc/ALb4P0r6ubwVPSGB2GvjeoMAJJImBG12TiaQ==", :src => "https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"}
    %link{:crossorigin => "anonymous", :href => "https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css", :integrity => "sha512-mSYUmp1HYZDFaVKK//63EcZq4iFWFjxSL+Z3T/aCt4IO9Cejm03q3NKKYN6pFQzY0SBOr8h+eCIAZHPXcpZaNw==", :rel => "stylesheet"}/

    %script{defer: 'defer', src: "https://maps.googleapis.com/maps/api/js?key=#{ENV['GOOGLE_MAP_KEY']}&callback=initMap&libraries=&v=weekly"}
    = javascript_include_tag 'highcharts', 'data-turbolinks-track': false

    = yield(:gon_data)
    = csrf_meta_tags

    / HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    / WARNING: Respond.js doesn't work if you view the page via file://
    /[if lt IE 9]
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  %body
    .se-pre-con{style: "display: none;"}
    .wrapper
      .box
        .canvas-wrapper.main-wrapper
          .hamburger.hamburger--sticky
            %span 
          #sidebar.sidebar-offcanvas.left-column{:role => "menu"}
            = render 'shared/profile_left_nav'
          .right-column.page-contents-wrapper
            .wrapper
              .row.middle-content
                .main
                  .header-wrapper
                    = render 'shared/header'
                  = render 'shared/alerts' if flash.present?
                  .content.p-n
                    = yield
            .bg-sidebar-overlay
    = render 'shared/google_analytics'

  :scss
    .theme, .theme a{
      color: #f8af17!important;
    }
  :javascript
    $(window).load(function(){
      $("ol.progtrckr").each(function(){
        $(this).attr("data-progtrckr-steps", $(this).children("li").length);
      });
    })
