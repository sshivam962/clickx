- clients.each do |client|
  = render 'super_admin/clients/archived/client', client: client
.row.mt-md
  .col-md-4.col-lg-4.col-sm-12
    = page_entries_info clients
  .col-md-8.col-lg-8.col-sm-12.right_align
    = will_paginate clients, params: {name: params[:name]}
:javascript
  $('.pagination a').attr('data-remote', 'true');
