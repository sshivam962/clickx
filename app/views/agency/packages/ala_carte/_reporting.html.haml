- plan = plans['ad_hoc_reports']&.first
- if plan.present?
  .ala_carte-list--item.reporting{"data-filter" => "reporting"}
    .plan-card
      .plan-card--header
        .f-s-22.fw-sb.text-black Ad-Hoc Reports
        .plan-card--price
          = in_currency(plan.amount, current_agency.display_currency)
        .plan-card--price-slashed
          MSRP: #{in_currency(plan.min_recommended_price, current_agency.display_currency)}
      .plan-card--body
        %ul.plan-card-list
          %li
            %i.material-icons.text-info.tick-r.f-s-20 check
            %span 1 Custom Made Report
          %li
            %i.material-icons.text-info.tick-r.f-s-20 check
            %span Google Analytics Data
          %li
            %i.material-icons.text-info.tick-r.f-s-20 check
            %span Google Search Console
          %li
            %i.material-icons.text-info.tick-r.f-s-20 check
            %span Google Ads Data
          %li
            %i.material-icons.text-info.tick-r.f-s-20 check
            %span Facebook Ads Data
          %li
            %i.material-icons.text-info.tick-r.f-s-20 check
            %span Technical SEO Reports
          %li
            %i.material-icons.text-info.tick-r.f-s-20 check
            %span Third Party Research Data
      - unless @is_preview
        .plan-card--footer
          %a.btn.btn-info.btn-raised.btn-block.btn-lg.m-n{href: agency_checkout_path(package: 'ala_carte', plan: plan.key)}
            Get Started

