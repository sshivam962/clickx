        = f.file_field :file
      -# %a{"data-gallery" => "gallery", "data-toggle" => "lightbox", href: f.object.file.url}
      -#   = image_tag f.object.file.url || '', class: 'descriptive_preview'
      .preview-thumb-buttons
        .preview-thumb-button
          = link_to_remove_association "<i class='fa fa-times'></i>".html_safe, f