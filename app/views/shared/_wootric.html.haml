- if Rails.env.production? && current_user.present? && current_user.business_user? && current_user.normal? && current_business&.clickx?
  :javascript
    wootric_survey_immediately = true; // Shows survey immediately for testing purposes.  TODO: Comment out for production.
    window.wootricSettings = {
      email: "#{current_user.email}",
      created_at: "#{(current_user.invitation_accepted_at || current_user.created_at).to_i}",
      account_token: 'NPS-4a2ec3b1' // This is your unique account token.
    };
    window.wootric('run');
