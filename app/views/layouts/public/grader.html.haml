!!!
%html
  %head
    %title
      = yield(:title) || @grader_report&.domain

    = stylesheet_link_tag 'themeX_base', media: 'all', 'data-turbolinks-track': false
    = stylesheet_link_tag "https://www.authy.com/form.authy.min.css"
    = stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery-minicolors/2.3.4/jquery.minicolors.min.css'
    = javascript_include_tag "https://www.authy.com/form.authy.min.js"
    = javascript_include_tag 'themeX_base', 'data-turbolinks-track': false
    = javascript_include_tag 'jquery.card', 'data-turbolinks-track': false
    = javascript_include_tag 'highcharts', 'data-turbolinks-track': false
    = javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery-minicolors/2.3.4/jquery.minicolors.min.js'
    = csrf_meta_tags
    = render partial: "layouts/favicon", locals: { agency_logo: @agency&.favicon }
    = render 'layouts/themeX/fonts'
    = render 'layouts/themeX/ie_support'

    = yield(:custom_assets)

  %body.sidebar-scroll.ui-2020.slidein-nav
    #wrapper
      #layout-static
        .static-content-wrapper
          .static-content
            .page-content.page-content--grader.grader-preloader
              = yield
