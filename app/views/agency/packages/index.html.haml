- content_for :title do
  Plans | Clickx
.ui-2020
  .page-content
    .page-heading.pt-md.pb-md
      .d-flex.align-items-center.justify-content-between.flex-wrap
        .col-auto
          %h1
            %i.clickx-icon-package
            Plans
        .col-auto.text-lg-right
          %a.btn.btn-warning.btn-raised.m-n.f-w-lg{href: agency_fulfilment_courses_path}
            %span.d-flex.align-items-center
              %span.mr-3.d-inline-block
                %i.clickx-icon-Video
              %span.d-inline-block
              Training
    .container-fluid
      .panel
        .panel-body.panel-br-10.pt-0.pb-0
          .row.d-flex.flex-wrap
            - if current_agency.enabled_package?('generate_lead_strategy')
              .col-lg-6.col-sm-6.col-xs-12.pb-5
                %a.card.br-10.m-0.text-center.d-flex.flex-column.h-100{href: '/a/packages/generate_lead_strategy'}
                  .col-stretch.pt-4
                    %span.card--plan__icon.d-inline-block
                      %i.clickx-icon-Strategize
                    %h4 Digital Marketing Strategy
                    %span.d-block.p-b-20
                  %div.py-3
                    %span.bg-dark.d-block.p-3.br-8.fs-xs View Plan
            
            - if current_agency.enabled_package?('local_seo')
