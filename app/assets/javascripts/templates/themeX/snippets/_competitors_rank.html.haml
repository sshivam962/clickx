%md-dialog.modal-dialog.clx-modal.modal-darkstone-blue-bg.modal-compressed{ 'aria-label': 'Competitors Rank' }
  %form
    .modal-header
      %button.close{ 'aria-label': 'Close', 'data-dismiss': 'modal', 'type': 'button',
      'ng-click': 'closeModal()' }
        %span.fa.fa-times-circle-o{ 'aria-hidden': 'true' }
      %h4.modal-title
        Competitors Rank
    %md-dialog-content
      .md-dialog-content
        .col-md-12
          .tab-container
            .tab-content{ 'ng-if': 'competitor_ranks.length == 0' }
              %h4.text-muted.text-center
                Nothing to show, please check back again later.
            .table-responsive{ 'ng-if': 'competitor_ranks.length > 0'}
              %table.table.table-hover.table-bordered.table-vam
                %thead
                  %tr
                    %th
                      %b URL
                    %th.text-center
                      %b Google Rank
                %tbody
                  %tr{'ng-repeat':"rank in competitor_ranks"}
                    %td
                      %a.text-info{'ng-href': "{{rank.url}}", target: '_blank' }
                        {{ rank.url }}
                    %td.text-center {{ rank.rank }}    
    %md-dialog-actions{layout:'row' }
      %button.btn.btn-default.btn-raised{"data-dismiss" => "modal",
      :type => "button","ng-click":"closeModal($event)"} Close