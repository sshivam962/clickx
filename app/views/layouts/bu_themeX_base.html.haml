!!!
%html{'ng-app':"clickxApp"}
  %head
    = render 'layouts/themeX/meta_tags'
    %title{"ng-bind":"title || 'Dashboard'"}
      = yield(:title)

    = stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/w3-css/4.1.0/3/w3.min.css'
    = stylesheet_link_tag 'themeX_base', media: 'all', 'data-turbolinks-track': false
    = stylesheet_link_tag "https://www.authy.com/form.authy.min.css"
    = javascript_include_tag "https://www.authy.com/form.authy.min.js"
    = javascript_include_tag "https://disutgh7q0ncc.cloudfront.net/beacon.js"
    = javascript_include_tag 'themeX_base', 'data-turbolinks-track': false
    = javascript_include_tag 'jquery.card', 'data-turbolinks-track': false
    = yield(:custom_assets)
    = javascript_include_tag 'country_states', 'data-turbolinks-track': false
    = csrf_meta_tags
    = stylesheet_link_tag 'https://cdn.datatables.net/1.11.4/css/jquery.dataTables.min.css'
    = javascript_include_tag "https://cdn.datatables.net/1.11.4/js/jquery.dataTables.min.js"
    = javascript_include_tag "https://cdn.datatables.net/1.12.1/js/dataTables.bootstrap.min.js"
    = stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/css/select2.min.css'
    = javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/js/select2.min.js"
    = javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/cleave.js/1.4.2/cleave.min.js"


    = stylesheet_link_tag 'https://cdn.datatables.net/1.11.4/css/jquery.dataTables.min.css'
    = javascript_include_tag "https://cdn.datatables.net/1.11.4/js/jquery.dataTables.min.js"
    %link{rel: "stylesheet", href: "https://www.w3schools.com/w3css/4/w3.css"}
    %link{href: 'https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css', rel: 'stylesheet', type: 'text/css'}
    %script{src: 'https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js', type: 'text/javascript'}
    %script{src: 'https://cdn.jsdelivr.net/npm/underscore@1.13.2/underscore-umd-min.js'}
    %script{src: "https://maps.googleapis.com/maps/api/js?key=#{ENV['GOOGLE_MAP_KEY']}&libraries=places"}

    %link{crossorigin: 'anonymous', href: 'https://cdnjs.cloudflare.com/ajax/libs/jquery-tagsinput/1.3.6/jquery.tagsinput.min.css', integrity: 'sha256-j5X3Yk4AwOdSE1go4GG6eQ/Bq0+gY2STs+GlULjQ+y8=', rel: 'stylesheet'}
    %script{crossorigin: 'anonymous', integrity: 'sha256-IkytFcY/Z/rLvRE9rYyZvybaitAWr1y2jS/+eyxXky8=', src: 'https://cdnjs.cloudflare.com/ajax/libs/jquery-tagsinput/1.3.6/jquery.tagsinput.min.js'}

    %script{src: 'https://code.highcharts.com/highcharts.js'}

    = render 'layouts/themeX/fonts'
    = render 'layouts/themeX/ie_support'
    / = render 'layouts/themeX/freshwidget'
    = render 'shared/google_analytics'
    = render 'shared/wootric'
    - if white_labeled_client? && current_tenent.logo.present?
      = favicon_link_tag asset_path(current_tenent.logo), rel: 'icon', type: "image/x-icon"
  %body.sidebar-scroll{'ng-controller':'Home'}
    = render 'layouts/themeX/flash'
    = render 'layouts/themeX/bu_header'
    #wrapper.master-wrapper
      #layout-static
        = render 'layouts/themeX/bu_sidebar'
        .static-content-wrapper
          .static-content
            .page-content
              = yield

    = render 'shared/vapid'
    = render 'shared/adblock_alert'
    = render 'shared/feedback_modal'
