          margin: 0;
          font-family: 'Montserrat', sans-serif;
      }

      table {
          border-collapse: collapse;
      }

      h1,
      h2,
      h3,
      h4,
      h5,
      h6 {
          font-family: 'Montserrat', sans-serif;
      }

      .table {
